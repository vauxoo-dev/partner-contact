<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="res_company_form" model="ir.ui.view">
            <field name="name">res.country.rma.config.form.view</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@name='act_discover_fonts']" position="after">
                    <newline/>
                    <group>
                        <field name="address_format_id" groups="base.group_no_one"/>
                        <button name="test_address_format_id"  groups="base.group_no_one" string="Test Format Address Picked view" class="oe_link"/>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</openerp>
